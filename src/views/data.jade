extends layout

block content
	.row
		.small-12.columns
			h1= locals.title
		hr
	for value, i in locals.values
		.row
			.small-8.large-3.columns
				h3.subheader Messwert #{i+1}:
			.small-4.large-1.pull-9.columns
				h3(id='value#{i}')=value

block script
	script(src='/socket.io/socket.io.js')
	script(src='js/data/data.js')