{
  "app": {
    "width": 400,
    "height": 500,
    "autoHideMenuBar": true,
    "acceptFirstMouse": true,
    "webPreferences": {
      "webSecurity": true
    },
    "x": 50,
    "y": 50
  },
  "server": {
    "port": {
      "http": 80,
      "https": 443
    },
    "auth": {
      "required": true,
      "suffix": "@fz-juelich.de",
      "ldap": {
        "url": "ibn-net.kfa-juelich.de",
        "baseDN": "dc=ibn-net,dc=kfa-juelich,dc=de"
      }
    },
    "logs": {
      "http_server_log": "./logs/http-error.log",
      "server_log": "./logs/server-error.log"
    }
  },
  "userConfigFiles": {
    "HNF-GDS": {
      "path": "\\\\ibn-vsrv\\HNF-GDS$\\Config\\HNF_GDS.json",
      "#path": ".\\defaults\\HNF\\HNF_GDS.json",      
      "renderer": "Alarmzustände"
    },
    "mK_Smt": {
      "path": "./defaults/mK_Smt/mK_Smt.json",
      "renderer": "Diagrammansicht"
    }
  },
  "renderer": {
    "Alarmzustände": {
      "icon": "juelich:alarm",
      "path": "alarm-view.jade"
    },
    "Diagrammansicht": {
      "icon": "juelich:graph",
      "path": "chart-view.jade"
    }
  },
  "database": {}
}
